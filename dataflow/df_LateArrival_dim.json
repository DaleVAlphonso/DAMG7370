{
	"name": "df_LateArrival_dim",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Late_arrival_DS",
						"type": "DatasetReference"
					},
					"name": "sourceDimCustomer"
				},
				{
					"dataset": {
						"referenceName": "Late_arrival_DS",
						"type": "DatasetReference"
					},
					"name": "sourceCustomer2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Late_arrival_DS",
						"type": "DatasetReference"
					},
					"name": "sinkCustomerDim"
				}
			],
			"transformations": [
				{
					"name": "lookup1"
				},
				{
					"name": "AlterRow1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          CUSTOMER_SK as decimal(38,0),",
				"          CUSTOMER_ID as decimal(38,0),",
				"          CUSTOMER_NAME as string,",
				"          DI_CREATEDDT as timestamp,",
				"          PID as string,",
				"          IS_INFERRED as boolean",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> sourceDimCustomer",
				"source(output(",
				"          CUSTOMERID as decimal(38,0),",
				"          CUSTOMERNAME as string,",
				"          BILLTO as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> sourceCustomer2",
				"sourceDimCustomer, sourceCustomer2 lookup(CUSTOMERID == CUSTOMER_ID,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"lookup1 alterRow(upsertIf(IS_INFERRED == true())) ~> AlterRow1",
				"AlterRow1 derive(IS_INFERRED = false(),",
				"          CUSTOMER_NAME = CUSTOMERNAME) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:false,",
				"     updateable:false,",
				"     upsertable:true,",
				"     keys:['CUSTOMER_ID'],",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          CUSTOMER_SK,",
				"          CUSTOMER_ID,",
				"          CUSTOMER_NAME,",
				"          DI_CREATEDDT,",
				"          PID,",
				"          IS_INFERRED",
				"     )) ~> sinkCustomerDim"
			]
		}
	}
}